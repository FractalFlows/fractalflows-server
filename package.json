{
  "name": "fractalflows-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "mkdir certs && echo \"${POSTGRES_CA}\" > certs/ca-certificate.crt && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:create": "yarn typeorm migration:generate -- -n",
    "typeorm:migration:run": "yarn typeorm migration:run"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.4.7",
    "@nestjs/apollo": "^10.1.6",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/graphql": "10.1.6",
    "@nestjs/mapped-types": "1.2.0",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/typeorm": "^9.0.1",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^7.19.0",
    "@sentry/tracing": "^7.19.0",
    "apollo-server-express": "^3.11.1",
    "bcrypt": "^5.1.0",
    "concat-stream": "^2.0.0",
    "connect-pg-simple": "^8.0.0",
    "connect-redis": "^6.0.0",
    "crypto": "^1.0.1",
    "ethers": "^5.7.2",
    "express-session": "^1.17.2",
    "graphql": "16.6.0",
    "graphql-upload": "14",
    "nft.storage": "^7.0.0",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "redis": "^4.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "session-file-store": "^1.5.0",
    "siwe": "^1.1.6",
    "slugify": "^1.6.4",
    "twit": "^2.2.11",
    "twitter-api-v2": "^1.12.9",
    "typeorm": "^0.3.10",
    "web3.storage": "^4.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/jest": "29.2.3",
    "@types/node": "^18.11.9",
    "@types/session-file-store": "^1.2.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-morph": "^16.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
